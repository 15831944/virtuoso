.EXPORT_ALL_VARIABLES:

TOP	:= @TOP@
HOME    := @TOP@
OPSYS	:= "@OPSYS@"
HOST	:= @host@
CPU	:= @host_cpu@
VENDOR  := @host_vendor@
OS	:= @host_os@
CCWARN	= @CCWARN@
CCOPT	= @CCOPT@
CCDEBUG	= @CCDEBUG@
CCLIBS	= @CCLIBS@
CCDEFS	= @CCDEFS@ @CCPLATFORMDEFS@
CCINCL	= -I$(TOP)/libsrc -I$(TOP)/libsrc/Dk @ccincl@
AM_LDFLAGS := @AM_LDFLAGS@
SYSLIBS	:= @LIBS@
LEXLIB  := @LEXLIB@
LDLIBS	= $(XOBJS) -L$(LIBDIR) $(XLIBS) $(LIBS) $(SYSLIBS)
LDRLIBS	= $(XOBJS) -L$(LIBDIR) $(XLIBS) $(LIBS) $(CCLIBS)
LIBTOOL = $(SHELL) $(TOP)/libtool
CC = @CC@
INSTALL = @INSTALL@
LEX    := @LEX@
YACC    := @YACC@
CPP    := @CPP@
WGET   := @WGET@

XCFLAGS	= $(DEFS) $(INCL)
VIRT_AM_CFLAGS = $(CCOPT) $(CCWARN) $(CCDEBUG) $(CCINCL) $(CCDEFS) $(XCFLAGS)

BUILD_OPTS = @BUILD_OPTS@ noudbcsdk

# System thread libraries
LIBTHR	= @LIBTHR@

# System readline libraries
LIBRDL	= @LIBRDL@

# System libraries for iODBC
LIBDL	= @LIBDL@

SRVRLIBS= \
        @TOP@/libsrc/Dk/libdksrv.la \
        @TOP@/libsrc/Xml.new/libxml.la \
        @TOP@/libsrc/plugin/libplugin.la \
        @TOP@/libsrc/langfunc/liblangfunc.la \
        @TOP@/libsrc/zlib/libz.la \
        @TOP@/libsrc/Tidy/libtidy.la \
        @TOP@/libsrc/util/libutil.la \
        @srvrlibs@ \
        @LIBS@
# there is a circular dep in dk1t<->thrs in need to be resolved
CLNTLIBS= \
	@TOP@/libsrc/Wi/libwic.la \
	@TOP@/libsrc/Dk/libdk1t.la \
	@TOP@/libsrc/Thread/libthrs.la \
	@TOP@/libsrc/zlib/libz.la \
	@TOP@/libsrc/util/libutil.la \
	@clntlibs@
LIBXML_FILE=@libxml_file@

MAKEVER:=$(TOP)/bin/makever

VERMAJ:=04.00
VERBMAJ:=28
VERBMIN:=16
PORT:=1111
#needed by sql_to_c.awk
PLSTATS:=PLDBG

JDK1:=@ac_jdk1_path@
JDK2:=@ac_jdk2_path@
JDK3:=@ac_jdk3_path@
