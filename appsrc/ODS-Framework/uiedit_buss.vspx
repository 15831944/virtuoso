                  <v:button name="user_sset_buss" value="Save" action="simple" xhtml_tabindex="21">
                    <v:on-post>
                      <![CDATA[
                        if(self.vc_is_valid = 0) return;

                        declare blat, blng any;
                        blat := blng := null;
                        if (length (self.b_lat.ufl_value) and length (self.b_lng.ufl_value))
                        {
                             blat := atof (self.b_lat.ufl_value);
                             blng := atof (self.b_lng.ufl_value);
                        };
                        declare _arr any;
                        _arr := WA_STR_PARAM(params,'sel_');
                        WA_USER_VISIBILITY (self.u_name, _arr, 2);

