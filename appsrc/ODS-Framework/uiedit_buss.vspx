    <v:before-data-bind>
      <![CDATA[
        declare _bindustr, _borg, _bjob, _baddress1, _baddress2, _bcode, _bcity, _bstate, _bcountry, _btzone,
                _bphone, _bmobile, _bregno, _bcareer, _bempltotal, _bvendor, _bservice, _bother, _bnetwork varchar;
        declare _bresume long varchar;

        SELECT WAUI_BINDUSTRY, WAUI_BORG, WAUI_BJOB, WAUI_BADDRESS1, WAUI_BADDRESS2, WAUI_BCODE, WAUI_BCITY,
               WAUI_BSTATE, WAUI_BCOUNTRY, WAUI_BTZONE, WAUI_BPHONE, WAUI_BMOBILE, WAUI_BREGNO,
               WAUI_BCAREER, WAUI_BEMPTOTAL, WAUI_BVENDOR, WAUI_BSERVICE, WAUI_BOTHER, WAUI_BNETWORK, WAUI_RESUME,
               WAUI_BLAT, WAUI_BLNG,WAUI_LATLNG_HBDEF
          INTO _bindustr, _borg, _bjob, _baddress1, _baddress2, _bcode, _bcity, _bstate, _bcountry, _btzone,
               _bphone, _bmobile, _bregno, _bcareer, _bempltotal, _bvendor, _bservice, _bother, _bnetwork, _bresume,
       	       self.blat, self.blng,self.blatlng_def

          FROM WA_USER_INFO, DB.DBA.SYS_USERS where WAUI_U_ID = U_ID and U_NAME = self.u_name;

        self.bindustr := _bindustr;
        self.borg := wa_utf8_to_wide (_borg);
        self.bjob := wa_utf8_to_wide (_bjob);
        self.baddress1 := wa_utf8_to_wide (_baddress1);
        self.baddress2 := wa_utf8_to_wide (_baddress2);
        self.bcode := _bcode;
        self.bcity := wa_utf8_to_wide (_bcity);
        self.bstate := _bstate;
        self.bcountry := _bcountry;
        self.btzone := _btzone;
        self.bphone := _bphone;
        self.bmobile := _bmobile;
        self.bregno := _bregno;
        self.bcareer := _bcareer;
        self.bempltotal := _bempltotal;
        self.bvendor := _bvendor;
        self.bservice := _bservice;
        self.bother := _bother;
        self.bnetwork := _bnetwork;
        self.bresume := wa_utf8_to_wide (_bresume);
        self.bempltotal := _bempltotal;

        declare tmp any;
        tmp := WA_USER_VISIBILITY(self.u_name);
        if (tmp is not null and length (tmp) >= 34 )
        {
          self.sav19 := tmp[19];
          self.sav20 := tmp[20];
          self.sav21 := tmp[21];
          self.sav22 := tmp[22];
          self.sav23 := tmp[23];
          self.sav24 := tmp[24];
          self.sav25 := tmp[25];
          self.sav26 := tmp[26];
          self.sav27 := tmp[27];
          self.sav28 := tmp[28];
          self.sav29 := tmp[29];
          self.sav30 := tmp[30];
          self.sav31 := tmp[31];
          self.sav32 := tmp[32];
          self.sav34 := tmp[34];
          self.sav47 := tmp[47];
          self.sav48 := tmp[48];
        }

      if (e.ve_is_post and e.ve_button = self.page_form)
        {
          self.usr_bussiness.vc_focus := 1;
          self.usr_bussiness.vc_set_childs_focus(1, e);
        }

      ]]>
    </v:before-data-bind>

