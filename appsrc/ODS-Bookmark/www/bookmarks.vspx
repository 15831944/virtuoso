            <v:before-render>
              <![CDATA[
                control.ufl_value := '<img src="image/impt_32.png" border="0"/>' || self.toolbarLabel('Import');
              ]]>
            </v:before-render>
          </v:url>
          <v:template type="simple" enabled="--case when self.toolbarEnable('import') then 0 else 1 end">
