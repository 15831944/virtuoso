<!-- Synopsis ......................... -->

<attributeGroup name='synopsis.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='synopsis' mixed='true'>
  <choice minOccurs='0' maxOccurs='unbounded'>
    <group ref='db:para.char.mix'/>
    <element ref='db:graphic'/>
    <element ref='db:mediaobject'/>
    <element ref='db:lineannotation'/>
    <element ref='db:co'/>
  </choice>
  <!-- (%para.char.mix; | graphic | mediaobject | lineannotation | co)* -->

  <attributeGroup ref='db:label.attrib'/>
  <attributeGroup ref='db:linespecific.attrib'/>
  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:synopsis.role.attrib'/>
  <!-- %local.synopsis.attrib; -->
</complexType>

<!-- LineAnnotation (defined in the Inlines section, below)-->

<!-- CmdSynopsis ...................... -->

<attributeGroup name='cmdsynopsis.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='cmdsynopsis'>
  <sequence>
    <choice minOccurs='1' maxOccurs='unbounded'>
      <element ref='db:command'/>
      <element ref='db:arg'/>
      <element ref='db:group'/>
      <element ref='db:sbr'/>
    </choice>
    <element ref='db:synopfragment' minOccurs='0' maxOccurs='unbounded'/>
  </sequence>
  <!-- ((command | arg | group | sbr)+, synopfragment*) -->

  <!-- Sepchar: Character that should separate command and all
       top-level arguments; alternate value might be e.g., &Delta; -->
  <attribute name='sepchar' type='string' use='default' value=' '/>
  <attributeGroup ref='db:label.attrib'/>
  <attribute name='cmdlength' type='integer'/>
  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:cmdsynopsis.role.attrib'/>
  <!-- %local.cmdsynopsis.attrib; -->
</complexType>

<attributeGroup name='arg.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='arg' mixed='true'>
  <choice minOccurs='0' maxOccurs='unbounded'>
    <element ref='db:arg'/>
    <element ref='db:group'/>
    <element ref='db:option'/>
    <element ref='db:synopfragmentref'/>
    <element ref='db:replaceable'/>
    <element ref='db:sbr'/>
  </choice>
  <!-- (#PCDATA
		| arg
		| group
		| option
		| synopfragmentref
		| replaceable
		| sbr)* -->

  <!-- Choice: Whether Arg must be supplied: Opt (optional to
       supply, e.g. [arg]; the default), Req (required to supply,
       e.g. {arg}), or Plain (required to supply, e.g. arg) -->
  <attribute name='choice' use='default' value='opt'>
    <simpleType>
      <restriction base='string'>
        <enumeration value='opt'/>
        <enumeration value='req'/>
        <enumeration value='plain'/>
      </restriction>
    </simpleType>
  </attribute>

  <!-- Rep: whether Arg is repeatable: Norepeat (e.g. arg without
       ellipsis; the default), or Repeat (e.g. arg...) -->
  <attribute name='rep' use='default' value='norepeat'>
    <simpleType>
      <restriction base='string'>
        <enumeration value='norepeat'/>
        <enumeration value='repeat'/>
      </restriction>
    </simpleType>
  </attribute>

  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:arg.role.attrib'/>
  <!-- %local.arg.attrib; -->
</complexType>

<attributeGroup name='group.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='group'>
  <choice minOccurs='1' maxOccurs='unbounded'>
    <element ref='db:arg'/>
    <element ref='db:group'/>
    <element ref='db:option'/>
    <element ref='db:synopfragmentref'/>
    <element ref='db:replaceable'/>
    <element ref='db:sbr'/>
  </choice>
  <!-- ((arg | group | option | synopfragmentref
		| replaceable | sbr)+) -->

  <!-- Choice: Whether Arg must be supplied: Opt (optional to
       supply, e.g. [arg]; the default), Req (required to supply,
       e.g. {arg}), or Plain (required to supply, e.g. arg) -->
  <attribute name='choice' use='default' value='opt'>
    <simpleType>
      <restriction base='string'>
        <enumeration value='opt'/>
        <enumeration value='req'/>
        <enumeration value='plain'/>
      </restriction>
    </simpleType>
  </attribute>

  <!-- Rep: whether Arg is repeatable: Norepeat (e.g. arg without
       ellipsis; the default), or Repeat (e.g. arg...) -->
  <attribute name='rep' use='default' value='norepeat'>
    <simpleType>
      <restriction base='string'>
        <enumeration value='norepeat'/>
        <enumeration value='repeat'/>
      </restriction>
    </simpleType>
  </attribute>

  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:group.role.attrib'/>
  <!-- %local.group.attrib; -->
</complexType>

<!-- Synopsis break -->
<attributeGroup name='sbr.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='sbr'>
  <!-- EMPTY -->
  <complexContent>
    <restriction base='xsd:anyType'>
      <attributeGroup ref='db:common.attrib'/>
      <attributeGroup ref='db:sbr.role.attrib'/>
      <!-- %local.sbr.attrib; -->
    </restriction>
  </complexContent>
</complexType>

<attributeGroup name='synopfragmentref.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='synopfragmentref' mixed='true'>
  <!-- (#PCDATA) -->
  <simpleContent>
    <extension base='string'>
      <!-- to SynopFragment of complex synopsis
           material for separate referencing -->
      <attributeGroup ref='db:linkendreq.attrib'/>
      <attributeGroup ref='db:common.attrib'/>
      <attributeGroup ref='db:synopfragmentref.role.attrib'/>
      <!-- %local.synopfragmentref.attrib; -->
    </extension>
  </simpleContent>
</complexType>

<attributeGroup name='synopfragment.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='synopfragment'>
  <choice minOccurs='1' maxOccurs='unbounded'>
    <element ref='db:arg'/>
    <element ref='db:group'/>
  </choice>
  <!-- ((arg | group)+) -->

  <attributeGroup ref='db:idreq.common.attrib'/>
  <attributeGroup ref='db:synopfragment.role.attrib'/>
  <!-- %local.synopfragment.attrib; -->
</complexType>

<!-- Command (defined in the Inlines section, below)-->
<!-- Option (defined in the Inlines section, below)-->
<!-- Replaceable (defined in the Inlines section, below)-->

<!-- FuncSynopsis ..................... -->

<attributeGroup name='funcsynopsis.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='funcsynopsis'>
  <choice minOccurs='1' maxOccurs='unbounded'>
    <element ref='db:funcsynopsisinfo'/>
    <element ref='db:funcprototype'/>
  </choice>
  <!-- ((funcsynopsisinfo | funcprototype)+) -->

  <attributeGroup ref='db:label.attrib'/>
  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:funcsynopsis.role.attrib'/>
  <!-- %local.funcsynopsis.attrib; -->
</complexType>

<attributeGroup name='funcsynopsisinfo.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='funcsynopsisinfo' mixed='true'>
  <choice minOccurs='0' maxOccurs='unbounded'>
    <group ref='db:cptr.char.mix'/>
    <element ref='db:lineannotation'/>
  </choice>
  <!-- (%cptr.char.mix; | lineannotation)* -->

  <attributeGroup ref='db:linespecific.attrib'/>
  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:funcsynopsisinfo.role.attrib'/>
  <!-- %local.funcsynopsisinfo.attrib; -->
</complexType>

<attributeGroup name='funcprototype.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='funcprototype'>
  <sequence>
    <element ref='db:funcdef'/>
    <choice minOccurs='1' maxOccurs='unbounded'>
      <element ref='db:void'/>
      <element ref='db:varargs'/>
      <element ref='db:paramdef'/>
    </choice>
  </sequence>
  <!-- (funcdef, (void | varargs | paramdef+)) -->

  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:funcprototype.role.attrib'/>
  <!-- %local.funcprototype.attrib; -->
</complexType>

<attributeGroup name='funcdef.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='funcdef' mixed='true'>
  <choice minOccurs='0' maxOccurs='unbounded'>
    <element ref='db:replaceable'/>
    <element ref='db:function'/>
  </choice>
  <!-- (#PCDATA
		| replaceable
		| function)* -->

  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:funcdef.role.attrib'/>
  <!-- %local.funcdef.attrib; -->
</complexType>

<attributeGroup name='void.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='void'>
  <!-- EMPTY -->
  <complexContent>
    <restriction base='xsd:anyType'>
      <attributeGroup ref='db:common.attrib'/>
      <attributeGroup ref='db:void.role.attrib'/>
      <!-- %local.void.attrib; -->
    </restriction>
  </complexContent>
</complexType>

<attributeGroup name='varargs.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='varargs'>
  <!-- EMPTY -->
  <complexContent>
    <restriction base='xsd:anyType'>
      <attributeGroup ref='db:common.attrib'/>
      <attributeGroup ref='db:varargs.role.attrib'/>
      <!-- %local.varargs.attrib; -->
    </restriction>
  </complexContent>
</complexType>

<!-- Processing assumes that only one Parameter will appear in a
     ParamDef, and that FuncParams will be used at most once, for
     providing information on the "inner parameters" for parameters that
     are pointers to functions. -->

<attributeGroup name='paramdef.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='paramdef' mixed='true'>
  <choice minOccurs='0' maxOccurs='unbounded'>
    <element ref='db:replaceable'/>
    <element ref='db:parameter'/>
    <element ref='db:funcparams'/>
  </choice>
  <!-- (#PCDATA
		| replaceable
		| parameter
		| funcparams)* -->

  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:paramdef.role.attrib'/>
  <!-- %local.paramdef.attrib; -->
</complexType>

<attributeGroup name='funcparams.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='funcparams' mixed='true'>
  <choice minOccurs='0' maxOccurs='unbounded'>
    <group ref='db:cptr.char.mix'/>
  </choice>
  <!-- (%cptr.char.mix;)* -->

  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:funcparams.role.attrib'/>
  <!-- %local.funcparams.attrib; -->
</complexType>

<!-- LineAnnotation (defined in the Inlines section, below)-->
<!-- Replaceable (defined in the Inlines section, below)-->
<!-- Function (defined in the Inlines section, below)-->
<!-- Parameter (defined in the Inlines section, below)-->

<!-- ClassSynopsis ..................... -->

<attributeGroup name='classsynopsis.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='classsynopsis'>
  <sequence>
    <choice minOccurs='1' maxOccurs='unbounded'>
      <element ref='db:ooclass'/>
      <element ref='db:oointerface'/>
      <element ref='db:ooexception'/>
    </choice>
    <choice minOccurs='0' maxOccurs='unbounded'>
      <element ref='db:classsynopsisinfo'/>
      <element ref='db:fieldsynopsis'/>
      <group ref='db:method.synop.class'/>
    </choice>
  </sequence>
  <!-- ((ooclass|oointerface|ooexception)+,
                         (classsynopsisinfo
                          |fieldsynopsis|%method.synop.class;)*) -->

  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:classsynopsis.role.attrib'/>
  <!-- %local.classsynopsis.attrib; -->
  <attribute name='language' type='string'/>
  <attribute name='class' use='default' value='class'>
    <simpleType>
      <restriction base='string'>
        <enumeration value='class'/>
        <enumeration value='interface'/>
      </restriction>
    </simpleType>
  </attribute>
</complexType>

<attributeGroup name='classsynopsisinfo.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='classsynopsisinfo' mixed='true'>
  <choice minOccurs='0' maxOccurs='1'>
    <group ref='db:cptr.char.mix'/>
    <element ref='db:lineannotation'/>
  </choice>
  <!-- (%cptr.char.mix; | lineannotation)* -->

  <attributeGroup ref='db:linespecific.attrib'/>
  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:classsynopsisinfo.role.attrib'/>
  <!-- %local.classsynopsisinfo.attrib; -->
</complexType>

