  <attributeGroup ref='db:ooexception.role.attrib'/>
  <!-- %local.ooexception.attrib; -->
</complexType>

<attributeGroup name='modifier.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='modifier'>
  <!-- (%smallcptr.char.mix;)* -->


  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:modifier.role.attrib'/>
  <!-- %local.modifier.attrib; -->
</complexType>

<attributeGroup name='interfacename.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='interfacename'>
  <!-- (%smallcptr.char.mix;)* -->


  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:interfacename.role.attrib'/>
