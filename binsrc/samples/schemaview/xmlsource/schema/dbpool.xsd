<!-- IndexTerms appear in the text flow for generating or linking an
     index. -->

<attributeGroup name='indexterm.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='indexterm'>
  <sequence>
    <element ref='db:primary' minOccurs='0' maxOccurs='1'/>
    <choice minOccurs='0' maxOccurs='1'>
      <sequence>
        <element ref='db:secondary'/>
        <choice minOccurs='0' maxOccurs='1'>
          <sequence>
            <element ref='db:tertiary'/>
            <choice minOccurs='0' maxOccurs='1'>
              <element ref='db:see'/>
              <element ref='db:seealso' minOccurs='1' maxOccurs='unbounded'/>
            </choice>
          </sequence>
          <element ref='db:see'/>
          <element ref='db:seealso' minOccurs='1' maxOccurs='unbounded'/>
        </choice>
        <element ref='db:see'/>
        <element ref='db:seealso' minOccurs='1' maxOccurs='unbounded'/>
      </sequence>
    </choice>
  </sequence>
  <!-- (primary?, ((secondary, ((tertiary, (see|seealso+)?)
		| see | seealso+)?) | see | seealso+)?) -->

  <!-- Scope: Indicates which generated indices the IndexTerm
       should appear in: Global (whole document set), Local (this
       document only), or All (both) -->
  <attribute name='scope'>
    <simpleType>
      <restriction base='string'>
        <enumeration value='all'/>
        <enumeration value='global'/>
        <enumeration value='local'/>
      </restriction>
    </simpleType>
  </attribute>

  <!-- Significance: Whether this IndexTerm is the most pertinent
       of its series (Preferred) or not (Normal, the default) -->
  <attribute name='significance' use='default' value='normal'>
    <simpleType>
      <restriction base='string'>
        <enumeration value='preferred'/>
        <enumeration value='normal'/>
      </restriction>
    </simpleType>
  </attribute>

  <!-- Class: Indicates type of IndexTerm; default is Singular,
       or EndOfRange if StartRef is supplied; StartOfRange value
       must be supplied explicitly on starts of ranges -->
  <attribute name='class'>
    <simpleType>
      <restriction base='string'>
        <enumeration value='singular'/>
        <enumeration value='startofrange'/>
        <enumeration value='endofrange'/>
      </restriction>
    </simpleType>
  </attribute>

  <!-- StartRef: ID of the IndexTerm that starts the indexing
       range ended by this IndexTerm -->
  <!-- FIXME: constrain -->
  <attribute name='startref' type='IDREF'/>

  <!-- Zone: IDs of the elements to which the IndexTerm applies,
       and indicates that the IndexTerm applies to those entire
       elements rather than the point at which the IndexTerm
      occurs -->
<!-- 	/*** BEG RUS/FIXME Tue May 22 16:24:15 2001 ***/
	Syntax error, multiply definition of attribute 
  <attribute name='startref' type='IDREFS'/> 
	/*** END RUS/FIXME Tue May 22 16:24:17 2001 ***/ -->

  <attributeGroup ref='db:pagenum.attrib'/>
  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:indexterm.role.attrib'/>
  <!-- %local.indexterm.attrib; -->
</complexType>

<attributeGroup name='primsecter.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='primary' mixed='true'>
  <choice minOccurs='0' maxOccurs='unbounded'>
