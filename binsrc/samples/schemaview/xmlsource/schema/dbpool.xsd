<!-- Procedure ........................ -->

<attributeGroup name='procedure.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='procedure'>
  <sequence>
    <group ref='db:formalobject.title.content' minOccurs='0' maxOccurs='1'/>
    <group ref='db:component.mix' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='db:step' minOccurs='1' maxOccurs='unbounded'/>
  </sequence>
  <!-- ((%formalobject.title.content;)?,
	(%component.mix;)*, step+) -->

  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:procedure.role.attrib'/>
  <!-- %local.procedure.attrib; -->
</complexType>

<attributeGroup name='step.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='step'>
  <sequence>
    <element ref='db:title' minOccurs='0' maxOccurs='1'/>
    <choice>
      <sequence>
        <group ref='db:component.mix' minOccurs='1' maxOccurs='unbounded'/>
        <sequence minOccurs='0' maxOccurs='1'>
          <element ref='db:substeps'/>
          <group ref='db:component.mix' minOccurs='0' maxOccurs='unbounded'/>
        </sequence>
      </sequence>
      <sequence>
        <element ref='db:substeps'/>
        <group ref='db:component.mix' minOccurs='0' maxOccurs='unbounded'/>
      </sequence>
    </choice>
  </sequence>
  <!-- (title?, (((%component.mix;)+, (substeps,
		(%component.mix;)*)?) | (substeps, (%component.mix;)*))) -->

  <!-- Performance: Whether the Step must be performed -->
  <!-- not #REQUIRED! -->
  <attribute name='performance'>
    <simpleType>
      <restriction base='string'>
        <enumeration value='optional'/>
        <enumeration value='required'/>
      </restriction>
    </simpleType>
  </attribute>

  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:step.role.attrib'/>
  <!-- %local.step.attrib; -->
</complexType>

<attributeGroup name='substeps.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='substeps'>
  <sequence>
    <element ref='db:step' minOccurs='1' maxOccurs='unbounded'/>
  </sequence>
  <!-- (step+) -->

  <!-- Performance: whether entire set of substeps must be performed -->
  <!-- not #REQUIRED! -->
  <attribute name='performance'>
    <simpleType>
      <restriction base='string'>
        <enumeration value='optional'/>
        <enumeration value='required'/>
      </restriction>
    </simpleType>
  </attribute>

  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:substeps.role.attrib'/>
  <!-- %local.substeps.attrib; -->
</complexType>

