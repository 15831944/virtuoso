        <enumeration value='linecolumn'/>
        <enumeration value='linerange'/>
        <enumeration value='linecolumnpair'/>
        <enumeration value='other'/>
      </restriction>
    </simpleType>
  </attribute>

  <!-- OtherUnits: User-defined units -->
  <attribute name='otherunits' type='NMTOKEN'/>

  <attribute name='coords' type='string' use='required'/>

  <attributeGroup ref='db:idreq.common.attrib'/>
  <attributeGroup ref='db:areaset.role.attrib'/>
  <!-- %local.areaset.attrib; -->
