</attributeGroup>

<complexType name='exceptionname'>
  <!-- (%smallcptr.char.mix;)* -->


  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:exceptionname.role.attrib'/>
  <!-- %local.exceptionname.attrib; -->
</complexType>

<attributeGroup name='fieldsynopsis.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
