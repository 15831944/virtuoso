<!-- Equations ........................ -->

<!-- This PE provides a mechanism for replacing equation content, -->
<!-- perhaps adding a new or different model (e.g., MathML) -->

<group name='equation.content'>
  <sequence>
    <element ref='db:alt' minOccurs='0' maxOccurs='1'/>
    <choice>
      <element ref='db:graphic' minOccurs='1' maxOccurs='unbounded'/>
      <element ref='db:mediaobject' minOccurs='1' maxOccurs='unbounded'/>
    </choice>
  </sequence>
  <!-- (alt?, (graphic+|mediaobject+)) -->
</group>

<group name='inlineequation.content'>
  <sequence>
    <element ref='db:alt' minOccurs='0' maxOccurs='1'/>
    <choice>
      <element ref='db:graphic' minOccurs='1' maxOccurs='unbounded'/>
      <element ref='db:inlinemediaobject' minOccurs='1' maxOccurs='unbounded'/>
    </choice>
  </sequence>
  <!-- (alt?, (graphic+|inlinemediaobject+)) -->
</group>

<attributeGroup name='equation.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='equation'>
  <sequence>
    <group ref='db:formalobject.title.content' minOccurs='0' maxOccurs='1'/>
    <choice>
      <element ref='db:informalequation'/>
      <group ref='db:equation.content'/>
    </choice>
  </sequence>
  <!-- ((%formalobject.title.content;)?, (informalequation |
		%equation.content;)) -->

  <attributeGroup ref='db:label.attrib'/>
  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:equation.role.attrib'/>
  <!-- %local.equation.attrib; -->
</complexType>

<attributeGroup name='informalequation.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='informalequation'>
  <sequence>
    <group ref='db:equation.content'/>
  </sequence>
  <!-- (%equation.content;)  -->

  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:informalequation.role.attrib'/>
  <!-- %local.informalequation.attrib; -->
</complexType>

<attributeGroup name='inlineequation.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='inlineequation'>
  <sequence>
    <group ref='db:inlineequation.content'/>
  </sequence>
  <!-- (%inlineequation.content;) -->

  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:inlineequation.role.attrib'/>
  <!-- %local.inlineequation.attrib; -->
</complexType>

<attributeGroup name='alt.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='alt'>
  <!-- (#PCDATA) -->
  <simpleContent>
    <extension base='string'>
      <attributeGroup ref='db:common.attrib'/>
      <attributeGroup ref='db:alt.role.attrib'/>
      <!-- %local.alt.attrib; -->
    </extension>
  </simpleContent>
</complexType>

<!-- Tables ........................... -->

<complexType name='table'>
  <complexContent>
    <extension base='tbl:tableType'>
      <sequence>
        <group ref='db:formalobject.title.content'/>
        <group ref='db:ndxterm.class' minOccurs='0' maxOccurs='1'/>
        <choice>
          <element ref='db:graphic' minOccurs='1' maxOccurs='unbounded'/>
          <element ref='db:mediaobject' minOccurs='1' maxOccurs='unbounded'/>
          <element ref='tbl:tgroup' minOccurs='1' maxOccurs='unbounded'/>
        </choice>
      </sequence>
      <!-- % tbl.table.mdl
             "((%formalobject.title.content;),
              (%ndxterm.class;)*,
              (graphic+|tgroup+))"-->
    </extension>
  </complexContent>
</complexType>

<complexType name='informaltable'>
  <complexContent>
    <extension base='tbl:tableType'>
      <choice>
        <element ref='db:graphic' minOccurs='1' maxOccurs='unbounded'/>
        <element ref='db:mediaobject' minOccurs='1' maxOccurs='unbounded'/>
        <element ref='tbl:tgroup' minOccurs='1' maxOccurs='unbounded'/>
      </choice>
    </extension>
  </complexContent>
</complexType>

