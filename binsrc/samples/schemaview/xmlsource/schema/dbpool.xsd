<attributeGroup name='objectdata.attrib'>
  <!-- FIXME: -->
  <!-- entityref	ENTITY		#IMPLIED -->
  <attribute name='fileref' type='string'/>
  <attribute name='format' type='NOTATION'/>
  <attribute name='srccredit' type='string'/>
  <!-- %local.objectdata.attrib;-->
</attributeGroup>

<attributeGroup name='videodata.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='videodata'>
  <!-- EMPTY -->
  <complexContent>
    <restriction base='xsd:anyType'>
      <!--Width: Same as CALS reprowid (desired width)-->
      <attribute name='width' type='string'/>

      <!--Depth: Same as CALS reprodep (desired depth)-->
      <attribute name='depth' type='string'/>

      <!--Align: Same as CALS hplace with 'none' removed; #IMPLIED means
          application-specific-->
      <attribute name='align'>
        <simpleType>
          <restriction base='string'>
            <enumeration value='left'/>
            <enumeration value='right'/>
            <enumeration value='center'/>
          </restriction>
        </simpleType>
      </attribute>

      <!--Scale: Conflation of CALS hscale and vscale-->
      <attribute name='scale' type='string'/>

      <!--Scalefit: Same as CALS scalefit-->
      <attribute name='scalefit' type='db:yesorno.attvals'/>

      <attributeGroup ref='db:common.attrib'/>
      <attributeGroup ref='db:objectdata.attrib'/>
      <!-- %local.videodata.attrib; -->
    </restriction>
  </complexContent>
</complexType>

<attributeGroup name='audiodata.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='audiodata'>
  <!-- EMPTY -->
  <complexContent>
    <restriction base='xsd:anyType'>
      <attributeGroup ref='db:common.attrib'/>
      <attributeGroup ref='db:objectdata.attrib'/>
      <!-- %local.audiodata.attrib; -->
      <attributeGroup ref='db:audiodata.role.attrib'/>
    </restriction>
  </complexContent>
</complexType>

<attributeGroup name='imagedata.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='imagedata'>
  <!-- EMPTY -->
  <complexContent>
    <restriction base='xsd:anyType'>
      <!--Width: Same as CALS reprowid (desired width)-->
      <attribute name='width' type='string'/>

      <!--Depth: Same as CALS reprodep (desired depth)-->
      <attribute name='depth' type='string'/>

      <!--Align: Same as CALS hplace with 'none' removed; #IMPLIED means
          application-specific-->
      <attribute name='align'>
        <simpleType>
          <restriction base='string'>
            <enumeration value='left'/>
            <enumeration value='right'/>
            <enumeration value='center'/>
          </restriction>
        </simpleType>
      </attribute>

      <!--Scale: Conflation of CALS hscale and vscale-->
      <attribute name='scale' type='string'/>

      <!--Scalefit: Same as CALS scalefit-->
      <attribute name='scalefit' type='db:yesorno.attvals'/>

      <attributeGroup ref='db:common.attrib'/>
      <attributeGroup ref='db:objectdata.attrib'/>
      <attributeGroup ref='db:imagedata.role.attrib'/>
      <!-- %local.videodata.attrib; -->
    </restriction>
  </complexContent>
</complexType>

<attributeGroup name='caption.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='caption'>
  <sequence>
    <group ref='db:textobject' minOccurs='0' maxOccurs='unbounded'/>
  </sequence>

  <attributeGroup ref='db:common.attrib'/>
  <!-- %local.caption.attrib; -->
  <attributeGroup ref='db:caption.role.attrib'/>
</complexType>

<attributeGroup name='mediaobjectco.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='mediaobjectco'>
  <sequence>
    <element ref='db:objectinfo' minOccurs='0' maxOccurs='1'/>
    <element ref='db:imageobjectco'/>
    <choice minOccurs='0' maxOccurs='unbounded'>
      <element ref='db:imageobjectco'/>
      <element ref='db:textobject'/>
    </choice>
  </sequence>
  <!-- (objectinfo?, imageobjectco,
			   (imageobjectco|textobject)*) -->

  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:mediaobjectco.role.attrib'/>
