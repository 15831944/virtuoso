<!DOCTYPE schema SYSTEM "/share/doctypes/xmlschema/XMLSchema.dtd" [
<!ENTITY % schemaAttrs "
	xmlns:tbl	CDATA	#IMPLIED
	xmlns:db	CDATA	#IMPLIED
">
]>
