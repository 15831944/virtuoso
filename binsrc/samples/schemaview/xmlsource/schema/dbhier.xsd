<!-- ...................................................................... -->
<!-- Entities for content models .......................................... -->

<group name='div.title.content'>
  <sequence>
    <element ref='db:title'/>
    <element ref='db:subtitle' minOccurs='0' maxOccurs='1'/>
    <element ref='db:titleabbrev' minOccurs='0' maxOccurs='1'/>
  </sequence>
</group>

<group name='bookcomponent.title.content'>
  <sequence>
    <element ref='db:title'/>
    <element ref='db:subtitle' minOccurs='0' maxOccurs='1'/>
    <element ref='db:titleabbrev' minOccurs='0' maxOccurs='1'/>
  </sequence>
</group>

<group name='sect.title.content'>
  <sequence>
    <element ref='db:title'/>
    <element ref='db:subtitle' minOccurs='0' maxOccurs='1'/>
    <element ref='db:titleabbrev' minOccurs='0' maxOccurs='1'/>
  </sequence>
</group>

<group name='refsect.title.content'>
  <sequence>
    <element ref='db:title'/>
    <element ref='db:subtitle' minOccurs='0' maxOccurs='1'/>
    <element ref='db:titleabbrev' minOccurs='0' maxOccurs='1'/>
  </sequence>
</group>

<group name='bookcomponent.content'>
  <choice>
    <sequence>
      <choice minOccurs='1' maxOccurs='unbounded'>
        <group ref='db:divcomponent.mix'/>
      </choice>
      <choice>
        <element ref='db:sect1' minOccurs='0' maxOccurs='unbounded'/>
        <group ref='db:refentry.class' minOccurs='0' maxOccurs='unbounded'/>
        <element ref='db:simplesect' minOccurs='0' maxOccurs='unbounded'/>
        <element ref='db:section' minOccurs='0' maxOccurs='unbounded'/>
      </choice>
    </sequence>
    <choice>
      <element ref='db:sect1' minOccurs='1' maxOccurs='unbounded'/>
      <group ref='db:refentry.class' minOccurs='1' maxOccurs='unbounded'/>
      <element ref='db:simplesect' minOccurs='1' maxOccurs='unbounded'/>
      <element ref='db:section' minOccurs='1' maxOccurs='unbounded'/>
    </choice>
  </choice>
  <!-- "((%divcomponent.mix;)+,
         (sect1*|(%refentry.class;)*|simplesect*|section*))
        | (sect1+|(%refentry.class;)+|simplesect+|section+)" -->
</group>

<!-- ...................................................................... -->
<!-- Set and SetInfo ...................................................... -->

<attributeGroup name='set.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='set'>
  <sequence>
    <group ref='db:div.title.content' minOccurs='0' maxOccurs='1'/>
    <element ref='db:setinfo' minOccurs='0' maxOccurs='1'/>
    <element ref='db:toc' minOccurs='0' maxOccurs='1'/>
    <group ref='db:book.class' minOccurs='1' maxOccurs='unbounded'/>
    <element ref='db:setindex' minOccurs='0' maxOccurs='1'/>
  </sequence>
  <!-- ((%div.title.content;)?, setinfo?, toc?, (%book.class;)+,
		setindex?) -->

  <!-- FPI: SGML formal public identifier -->
  <attribute name='fpi' type='string'/>

  <attributeGroup ref='db:status.attrib'/>
  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:set.role.attrib'/>
  <attributeGroup ref='db:local.set.attrib'/>
</complexType>

<attributeGroup name='setinfo.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='setinfo'>
  <complexContent>
    <extension base='db:infoContentModel'>
      <!-- Contents: IDs of the ToC, Books, and SetIndex that comprise
           the set, in the order of their appearance -->
      <attribute name='contents' type='IDREFS'/>
      <attributeGroup ref='db:common.attrib'/>
      <attributeGroup ref='db:setinfo.role.attrib'/>
      <attributeGroup ref='db:local.setinfo.attrib'/>
    </extension>
  </complexContent>
</complexType>

<!-- ...................................................................... -->
<!-- Book and BookInfo .................................................... -->

<attributeGroup name='book.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='book'>
  <sequence>
    <group ref='db:div.title.content' minOccurs='0' maxOccurs='1'/>
    <element ref='db:bookinfo' minOccurs='0' maxOccurs='1'/>
    <choice minOccurs='0' maxOccurs='unbounded'>
      <element ref='db:dedication'/>
      <element ref='db:toc'/>
      <element ref='db:lot'/>
      <element ref='db:glossary'/>
      <element ref='db:bibliography'/>
      <element ref='db:preface'/>
      <group ref='db:chapter.class'/>
      <element ref='db:reference'/>
      <element ref='db:part'/>
      <group ref='db:article.class'/>
      <group ref='db:appendix.class'/>
      <group ref='db:index.class'/>
      <element ref='db:colophon'/>
    </choice>
  </sequence>
  <!-- ((%div.title.content;)?, bookinfo?,
 		(dedication | toc | lot
 		| glossary | bibliography | preface
		| %chapter.class; | reference | part
		| %article.class;
 		| %appendix.class;
		| %index.class;
		| colophon)*) -->

  <!-- FPI: SGML formal public identifier -->
  <attribute name='fpi' type='string'/>

  <attributeGroup ref='db:label.attrib'/>
  <attributeGroup ref='db:status.attrib'/>
  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:book.role.attrib'/>
  <attributeGroup ref='db:local.book.attrib'/>
</complexType>

<attributeGroup name='bookinfo.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='bookinfo'>
  <complexContent>
    <extension base='db:infoContentModel'>
      <!-- Contents: IDs of the ToC, Books, and SetIndex that comprise
           the set, in the order of their appearance -->
      <attribute name='contents' type='IDREFS'/>
      <attributeGroup ref='db:common.attrib'/>
      <attributeGroup ref='db:bookinfo.role.attrib'/>
      <attributeGroup ref='db:local.bookinfo.attrib'/>
    </extension>
  </complexContent>
</complexType>

<!-- ...................................................................... -->
<!-- Dedication, ToC, and LoT ............................................. -->

<attributeGroup name='dedication.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='dedication'>
  <sequence>
    <group ref='db:sect.title.content' minOccurs='0' maxOccurs='1'/>
    <group ref='db:legalnotice.mix' minOccurs='1' maxOccurs='unbounded'/>
  </sequence>
  <!-- ((%sect.title.content;)?, (%legalnotice.mix;)+) -->

  <attributeGroup ref='db:status.attrib'/>
  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:dedication.role.attrib'/>
  <attributeGroup ref='db:local.dedication.attrib'/>
</complexType>

<attributeGroup name='colophon.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='colophon'>
  <sequence>
    <group ref='db:sect.title.content' minOccurs='0' maxOccurs='1'/>
    <group ref='db:textobject.mix' minOccurs='1' maxOccurs='unbounded'/>
  </sequence>
  <!-- ((%sect.title.content;)?, (%textobject.mix;)+) -->

  <attributeGroup ref='db:status.attrib'/>
  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:colophon.role.attrib'/>
  <attributeGroup ref='db:local.colophon.attrib'/>
</complexType>

<attributeGroup name='toc.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='toc'>
  <sequence>
    <group ref='db:bookcomponent.title.content' minOccurs='0' maxOccurs='1'/>
    <element ref='db:tocfront' minOccurs='0' maxOccurs='unbounded'/>
    <choice minOccurs='0' maxOccurs='unbounded'>
      <element ref='db:tocpart'/>
      <element ref='db:tocchap'/>
    </choice>
    <element ref='db:tocback' minOccurs='0' maxOccurs='unbounded'/>
  </sequence>
  <!-- ((%bookcomponent.title.content;)?, tocfront*,
		(tocpart | tocchap)*, tocback*) -->

  <attributeGroup ref='db:pagenum.attrib'/>
  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:toc.role.attrib'/>
  <attributeGroup ref='db:local.toc.attrib'/>
</complexType>

<attributeGroup name='tocfront.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='tocfront' mixed='true'>
  <choice minOccurs='0' maxOccurs='unbounded'>
    <group ref='db:para.char.mix'/>
  </choice>
  <!-- (%para.char.mix;)* -->

  <attributeGroup ref='db:label.attrib'/>
  <attributeGroup ref='db:linkend.attrib'/>
  <attributeGroup ref='db:pagenum.attrib'/>
  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:tocfront.role.attrib'/>
  <!-- %local.tocfront.attrib; -->
</complexType>

<attributeGroup name='tocentry.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='tocentry' mixed='true'>
  <choice minOccurs='0' maxOccurs='unbounded'>
    <group ref='db:para.char.mix'/>
  </choice>
  <!-- (%para.char.mix;)* -->

  <attributeGroup ref='db:linkend.attrib'/>
  <attributeGroup ref='db:pagenum.attrib'/>
  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:tocentry.role.attrib'/>
  <!-- %local.tocentry.attrib; -->
</complexType>

<attributeGroup name='tocpart.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='tocpart'>
  <sequence>
    <element ref='db:tocentry' minOccurs='1' maxOccurs='unbounded'/>
    <element ref='db:tocchap' minOccurs='0' maxOccurs='unbounded'/>
  </sequence>
  <!-- (tocentry+, tocchap*) -->

  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:tocpart.role.attrib'/>
  <!-- %local.tocpart.attrib; -->
</complexType>

<attributeGroup name='tocchap.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='tocchap'>
  <sequence>
    <element ref='db:tocentry' minOccurs='1' maxOccurs='unbounded'/>
    <element ref='db:toclevel1' minOccurs='0' maxOccurs='unbounded'/>
  </sequence>
  <!-- (tocentry+, toclevel1*) -->

  <attributeGroup ref='db:label.attrib'/>
  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:tocchap.role.attrib'/>
  <!-- %local.tocchap.attrib; -->
</complexType>

<attributeGroup name='toclevel1.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='toclevel1'>
  <sequence>
    <element ref='db:tocentry' minOccurs='1' maxOccurs='unbounded'/>
    <element ref='db:toclevel2' minOccurs='0' maxOccurs='unbounded'/>
  </sequence>
  <!-- (tocentry+, toclevel2*) -->

  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:toclevel1.role.attrib'/>
  <!-- %local.toclevel1.attrib; -->
</complexType>

<attributeGroup name='toclevel2.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='toclevel2'>
  <sequence>
    <element ref='db:tocentry' minOccurs='1' maxOccurs='unbounded'/>
    <element ref='db:toclevel3' minOccurs='0' maxOccurs='unbounded'/>
  </sequence>
  <!-- (tocentry+, toclevel3*) -->

  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:toclevel2.role.attrib'/>
  <!-- %local.toclevel2.attrib; -->
</complexType>

<attributeGroup name='toclevel3.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='toclevel3'>
  <sequence>
    <element ref='db:tocentry' minOccurs='1' maxOccurs='unbounded'/>
    <element ref='db:toclevel4' minOccurs='0' maxOccurs='unbounded'/>
  </sequence>
  <!-- (tocentry+, toclevel4*) -->

  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:toclevel3.role.attrib'/>
  <!-- %local.toclevel3.attrib; -->
</complexType>

<attributeGroup name='toclevel4.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='toclevel4'>
  <sequence>
    <element ref='db:tocentry' minOccurs='1' maxOccurs='unbounded'/>
    <element ref='db:toclevel5' minOccurs='0' maxOccurs='unbounded'/>
  </sequence>
  <!-- (tocentry+, toclevel5*) -->

  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:toclevel4.role.attrib'/>
  <!-- %local.toclevel4.attrib; -->
</complexType>

<attributeGroup name='toclevel5.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='toclevel5'>
  <sequence>
    <element ref='db:tocentry' minOccurs='1' maxOccurs='unbounded'/>
  </sequence>
  <!-- (tocentry+) -->

  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:toclevel5.role.attrib'/>
  <!-- %local.toclevel5.attrib; -->
</complexType>

<attributeGroup name='tocback.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='tocback' mixed='true'>
  <choice minOccurs='0' maxOccurs='unbounded'>
    <group ref='db:para.char.mix'/>
  </choice>
  <!-- (%para.char.mix;)* -->

  <attributeGroup ref='db:label.attrib'/>
  <attributeGroup ref='db:linkend.attrib'/>
  <attributeGroup ref='db:pagenum.attrib'/>
  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:tocback.role.attrib'/>
  <!-- %local.tocback.attrib; -->
</complexType>

<attributeGroup name='lot.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='lot'>
  <sequence>
    <group ref='db:bookcomponent.title.content' minOccurs='0' maxOccurs='1'/>
    <element ref='db:lotentry' minOccurs='0' maxOccurs='unbounded'/>
  </sequence>
  <!-- ((%bookcomponent.title.content;)?, lotentry*) -->

  <attributeGroup ref='db:label.attrib'/>
  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:lot.role.attrib'/>
  <!-- %local.lot.attrib; -->
</complexType>

<attributeGroup name='lotentry.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='lotentry' mixed='true'>
  <choice minOccurs='0' maxOccurs='unbounded'>
    <group ref='db:para.char.mix'/>
  </choice>
  <!-- (%para.char.mix;)* -->

  <!-- SrcCredit: Information about the source of the entry,
       as for a list of illustrations -->
  <attribute name='srccredit' type='string'/>
  <attributeGroup ref='db:pagenum.attrib'/>
  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:linkend.attrib'/>
  <attributeGroup ref='db:lotentry.role.attrib'/>
  <!-- %local.lotentry.attrib; -->
</complexType>

<!-- ...................................................................... -->
<!-- Appendix, Chapter, Part, Preface, Reference, PartIntro ............... -->

<attributeGroup name='appendix.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='appendix'>
  <sequence>
    <element ref='db:appendixinfo' minOccurs='0' maxOccurs='1'/>
    <group ref='db:bookcomponent.title.content'/>
    <group ref='db:nav.class' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='db:tocchap' minOccurs='0' maxOccurs='1'/>
    <group ref='db:bookcomponent.content'/>
    <group ref='db:nav.class' minOccurs='0' maxOccurs='unbounded'/>
  </sequence>
  <!-- (appendixinfo?,
                     (%bookcomponent.title.content;),
                     (%nav.class;)*,
                     tocchap?,
                     (%bookcomponent.content;),
                     (%nav.class;)*) -->

  <attributeGroup ref='db:label.attrib'/>
  <attributeGroup ref='db:status.attrib'/>
  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:appendix.role.attrib'/>
  <!-- %local.appendix.attrib; -->
</complexType>

<attributeGroup name='chapter.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='chapter'>
  <sequence>
    <element ref='db:chapterinfo' minOccurs='0' maxOccurs='1'/>
    <group ref='db:bookcomponent.title.content'/>
    <group ref='db:nav.class' minOccurs='0' maxOccurs='unbounded'/>
    <element ref='db:tocchap' minOccurs='0' maxOccurs='1'/>
    <group ref='db:bookcomponent.content'/>
    <group ref='db:nav.class' minOccurs='0' maxOccurs='unbounded'/>
  </sequence>
  <!-- (chapterinfo?,
                    (%bookcomponent.title.content;),
                    (%nav.class;)*,
                    tocchap?,
                    (%bookcomponent.content;),
                    (%nav.class;)*) -->

  <attributeGroup ref='db:label.attrib'/>
  <attributeGroup ref='db:status.attrib'/>
  <attributeGroup ref='db:common.attrib'/>
  <attributeGroup ref='db:chapter.role.attrib'/>
  <!-- %local.chapter.attrib; -->
</complexType>

<!-- Note that Part was to have its content model reduced in V4.1.2.  This
change will not be made after all. -->

<attributeGroup name='part.role.attrib'>
  <attributeGroup ref='db:role.attrib'/>
</attributeGroup>

<complexType name='part'>
  <sequence>
    <element ref='db:partinfo' minOccurs='0' maxOccurs='1'/>
    <group ref='db:bookcomponent.title.content'/>
    <element ref='db:partintro' minOccurs='0' maxOccurs='1'/>
    <group ref='db:partcontent.mix' minOccurs='1' maxOccurs='unbounded'/>
  </sequence>
  <!-- (partinfo?, (%bookcomponent.title.content;), partintro?,
		(%partcontent.mix;)+) -->

