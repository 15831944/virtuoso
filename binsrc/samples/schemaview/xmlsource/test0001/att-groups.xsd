<?xml version="1.0"?>
<!DOCTYPE xs:schema SYSTEM "XMLSchema.dtd">
<xs:schema>
<xs:attributeGroup name="grp1">
	<xs:attribute name="att11" type="xs:string"/>
	<xs:attribute name="att12" type="xs:string"/>
	<xs:attribute name="att13" type="xs:string"/>
	<xs:attribute name="att14" type="xs:string"/>
</xs:attributeGroup>
<xs:attributeGroup name="grp2">
	<xs:attribute name="att21" type="xs:string"/>
	<xs:attribute name="att22" type="xs:string"/>
	<xs:attribute name="att23" type="xs:string"/>
	<xs:attribute name="att24" type="xs:string"/>
</xs:attributeGroup>
<xs:attributeGroup name="grp3">
	<xs:attribute name="att3" type="xs:string"/>
	<xs:attributeGroup ref="grp2"/>
</xs:attributeGroup>
<xs:attributeGroup name="attgrp">
	<xs:attributeGroup ref="grp3"/>
</xs:attributeGroup>

<xs:complexType name="aEl0000">
  <xs:attributeGroup ref="grp1"/>
  <xs:attribute name="xxx" type="xs:string"/>
</xs:complexType>

<xs:complexType name="aElement">
  <xs:simpleContent>
    <xs:extension base="aEl0000">
	<xs:attributeGroup ref="attgrp"/>
    </xs:extension>
  </xs:simpleContent>
</xs:complexType>


<xs:element name="main">
 <xs:complexType>
  <xs:sequence>
   <xs:element ref="El" maxOccurs="unbounded"/>
  </xs:sequence>
 </xs:complexType>
</xs:element>



<!-- Elements -->
<xs:element name="El" type="aElement"/>
</xs:schema>	
