<?xml version="1.0"?>
<!DOCTYPE xs:schema SYSTEM "XMLSchema.dtd">
<xs:schema>

<xs:element name="testb">
 <xs:complexType>
  <xs:sequence>
   <xs:element ref="sub0" minOccurs="0"/>
   <xs:element ref="sub3" minOccurs="0"/>
   <xs:choice minOccurs="0" maxOccurs="1">
    <xs:element ref="sub1"/>
    <xs:element ref="sub2"/>
    <xs:group ref="appendix.class"/>
   </xs:choice>
  </xs:sequence>
 </xs:complexType>
</xs:element>

<xs:group name="appendix.class">
 <xs:choice>
  <xs:element ref="appendix"/>
 </xs:choice>
</xs:group>

<xs:element name="main">
  <xs:complexType>
    <xs:sequence>
	<xs:element ref="testb" maxOccurs="unbounded"/>
    </xs:sequence>
  </xs:complexType>
</xs:element>

<xs:element name="sub0" type="xs:string"/>
<xs:element name="sub1" type="xs:string"/>
<xs:element name="sub2" type="xs:string"/>
<xs:element name="sub3" type="xs:string"/>
<xs:element name="sub4" type="xs:string"/>
<xs:element name="appendix" type="xs:string"/>
</xs:schema>
