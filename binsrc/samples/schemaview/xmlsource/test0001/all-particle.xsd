<?xml version="1.0"?>
<!DOCTYPE schema SYSTEM "XMLSchema.dtd" [
 <!ENTITY % p "">
 <!ENTITY % s "">
 <!ENTITY % schemaAttrs "
        xmlns:r		CDATA	#IMPLIED
        xmlns:xipo	CDATA	#IMPLIED
 ">
]>	
<schema targetNamespace="http://www.example.com/Report"
        xmlns:r="http://www.example.com/Report"
        xmlns:xipo="http://www.example.com/IPO"
        elementFormDefault="qualified">

		
<!-- xmlns="http://www.w3.org/2001/XMLSchema" -->
<include schemaLocation="empty.xsd"/>
<complexType name='biblioentry'>
  <choice minOccurs='1' maxOccurs='unbounded'>
    <sequence>
	<element name="bla1" type="string"/>
	<element name="bla2" type="string"/>
    </sequence>
    <element ref="r:H1B"/>
    <group ref='db:bibliocomponent.mix'/>
  </choice>
</complexType>

<element name="H1B" type="string"/>

 <complexType name="hello">
  <all>
  	<element name="subhello" type="integer"/>
	<element name="subhello1" type="integer"/>
	<element name="subhello2" type="integer"/>
	<element name="subhello3" type="integer"/>
	<element name="subhello4" type="integer"/>
	<element name="subhello5" type="integer"/>
	<element name="subhello6" type="integer"/>
	<element name="subhello7" type="integer"/>
	<element ref="r:exthello" minOccurs="0"/>
  </all>
 </complexType>
 <element name="exthello" type="string"/>

</schema>
