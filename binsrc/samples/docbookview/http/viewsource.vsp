?>
<?vsp DBV.DBV.VSP_HEADER (path,params,lines, concat(_fullname, ' | Source text')); ?>
<PRE>
<?= _text ?>
</PRE>
<?vsp DBV.DBV.VSP_FOOTER (path,params,lines); ?>
