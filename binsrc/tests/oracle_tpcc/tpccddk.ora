
create table warehouse (
			w_id         integer,
			w_name       char(10),
			w_street_1   char(20),
			w_street_2   char(20),
			w_city       char(20),
			w_state      char(2),
			w_zip        char(9),
			w_tax        float,
			w_ytd        float);


create unique index warehouse_prime on warehouse (w_id);
     
     
     
create table district (
		       d_id         integer,
		       d_w_id       integer,
		       d_name       char(10),
		       d_street_1   char(20),
		       d_street_2   char(20),
		       d_city       char(20),
		       d_state      char(2),
		       d_zip        char(9),
		       d_tax        float,
		       d_ytd        float,
		       d_next_o_id  int );

create unique index district_prime on district(d_w_id,d_id);

create table customer (
		       c_id           integer,
		       c_d_id         integer ,
		       c_w_id         integer ,
		       c_first        char(16),
		       c_middle       char(2),
		       c_last         char(16),
		       c_street_1     char(20),
		       c_street_2     char(20),
		       c_city         char(20),
		       c_state        char(2),
		       c_zip          char(9),
		       c_phone        char(16),
		       c_since        date,
		       c_credit       char(2),
		       c_credit_lim   float,
		       c_discount     float,
		       c_balance      float,
		       c_ytd_payment  float,
		       c_cnt_payment  integer ,
		       c_cnt_delivery integer ,
		       c_data_1       char(250),
		       c_data_2       char(250));


create unique index customer_prime on customer(c_w_id,c_d_id,c_id);

create unique index customer_ncl on customer (c_w_id, c_d_id, c_last, c_first, c_id);

create table history (
		      h_c_id       integer,
		      h_c_d_id     integer ,
		      h_c_w_id     integer ,
		      h_d_id       integer ,
		      h_w_id       integer ,
		      h_date       date,
		      h_amount     float,
		      h_data       char(24));

create table new_order (
			no_o_id      integer,
			no_d_id      integer ,
			no_w_id      integer );

create unique index new_order_prime on new_order (no_w_id, no_d_id, no_o_id);

create table orders (
		     o_id         integer,
		     o_d_id       integer ,
		     o_w_id       integer ,
		     o_c_id       integer,
		     o_entry_d    date,
		     o_carrier_id integer ,
		     o_ol_cnt     integer ,
		     o_all_local  integer  );

create unique index orders_prime on orders (o_w_id, o_d_id,o_id);
     
create table order_line (
			 ol_o_id         integer,
			 ol_d_id         integer ,
			 ol_w_id         integer ,
			 ol_number       integer ,
			 ol_i_id         integer,
			 ol_supply_w_id  integer ,
			 ol_delivery_d   date,
			 ol_quantity     integer ,
			 ol_amount       float,
			 ol_dist_info    char(24));

create unique index order_line_prime on order_line (ol_w_id, ol_d_id, ol_o_id, ol_number);

create table item (
		   i_id            integer,
		   i_im_id         integer,
		   i_name          char(24),
		   i_price         float,
		   i_data          char(50));

create unique index item_prime on item (i_id);

create table stock (
		    s_i_id          integer,
		    s_w_id          integer ,
		    s_quantity      integer ,
		    s_dist_01       char(24),
		    s_dist_02       char(24),
		    s_dist_03       char(24),
		    s_dist_04       char(24),
		    s_dist_05       char(24),
		    s_dist_06       char(24),
		    s_dist_07       char(24),
		    s_dist_08       char(24),
		    s_dist_09       char(24),
		    s_dist_10       char(24),
		    s_ytd           integer,
		    s_cnt_order     integer ,
		    s_cnt_remote    integer ,
		    s_data          char(50) );

create unique index stock_prime on stock(s_i_id, s_w_id);

exit;
